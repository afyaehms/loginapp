<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.openmrs.module.loginapp.domain">
	<class name="LocationRole" table="afyaehms_location_role">
		<id name="id" type="java.lang.Integer">
			<column name="location_role_id" />
			<generator class="identity" />
		</id>
		<property name="preferred" type="java.lang.Boolean">
			<column name="preferred" />
		</property>
		<one-to-one name="role" class="org.openmrs.Role"></one-to-one>
		<one-to-one name="location" class="org.openmrs.Location"></one-to-one>
	</class>
</hibernate-mapping>