<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="org.openmrs.module.loginapp.domain.LocationRoleMapping" table="afyaehms_location_role_map">
		<id name="id" type="java.lang.Integer">
			<column name="location_role_id" />
			<generator class="identity" />
		</id>
		<property name="preferred" type="java.lang.Boolean">
			<column name="preferred" />
		</property>
		<property name="roleName" type="java.lang.String" not-null="true">
			<column name="role_name"></column>
		</property>
		<many-to-one name="location" column="location_id" class="org.openmrs.Location" not-null="true"/>
	</class>
</hibernate-mapping>